{
	"name": "OpenFin Web Layouts",
	"short_name": "OpenFinWebLayouts",
	"start_url": "./platform/provider.html",
	"display": "standalone",
	"background_color": "#fff",
	"description": "An example showing a implementation of the OpenFin Web Layouts Feature.",
	"icons": [
		{
			"src": "common/images/icon-blue.png",
			"sizes": "72x72",
			"type": "image/png"
		}
	],
	"related_applications": [],
